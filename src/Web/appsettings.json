{
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ChatSupportDb;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "QueueSettings": {
        "MaxConcurrentChatsPerAgent": 10,
        "QueueCapacityMultiplier": 1.5,
        "InactivityThresholdSeconds": 3
    }
